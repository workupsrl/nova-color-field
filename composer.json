{
  "name": "workup/nova-color-field",
  "description": "A Laravel Nova Color Picker field.",
  "keywords": [
    "laravel",
    "nova",
    "color picker",
    "field"
  ],
  "license": "MIT",
  "repositories": [
    {
      "type": "composer",
      "url": "https://nova.laravel.com"
    }
  ],
  "require": {
    "php": ">=8.0",
    "laravel/nova": "^4.0",
    "ext-json": "*"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^2",
    "orchestra/testbench": "^6",
    "roave/security-advisories": "dev-latest"
  },
  "autoload": {
    "psr-4": {
      "Workup\\NovaColorField\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Workup\\NovaColorField\\ColorFieldServiceProvider"
      ]
    }
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
